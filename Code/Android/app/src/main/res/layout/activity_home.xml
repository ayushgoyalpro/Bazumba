<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimaryDark"
    tools:context=".activities.Home">


    <ListView
        android:id="@+id/ListView1"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_marginTop="75dp"
        android:layout_marginBottom="63dp"
        android:divider="@android:color/transparent"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:dividerHeight="20dp"
        android:scrollbars="none"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="75dp"
        android:background="@color/colorPrimaryDark"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/montserrat_bold"
        android:text="HOME"
        android:textColor="#000000"
        android:textSize="32sp"
        app:layout_constraintBottom_toBottomOf="@+id/view"
        app:layout_constraintEnd_toEndOf="@+id/view"
        app:layout_constraintStart_toStartOf="@+id/view"
        app:layout_constraintTop_toTopOf="@+id/view" />

    <Button
        android:id="@+id/menuBT"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginEnd="96dp"
        android:background="@drawable/menuiconhome"
        app:layout_constraintBottom_toBottomOf="@+id/textView9"
        app:layout_constraintEnd_toStartOf="@+id/textView9"
        app:layout_constraintTop_toTopOf="@+id/textView9"
        android:onClick="MenuClick"/>

    <Button
        android:id="@+id/notificationIcon"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="108dp"
        android:background="@drawable/notificationicon"
        app:layout_constraintBottom_toBottomOf="@+id/textView9"
        app:layout_constraintStart_toEndOf="@+id/textView9"
        app:layout_constraintTop_toTopOf="@+id/textView9"/>

    <View
        android:id="@+id/view4"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@color/colorPrimary"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <View
        android:id="@+id/view5"
        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:background="#000000"
        app:layout_constraintBottom_toTopOf="@+id/view4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/homeIcon"
        android:layout_width="78dp"
        android:layout_height="78dp"
        android:background="@drawable/homeicon"
        app:layout_constraintStart_toStartOf="@id/menuBT"
        app:layout_constraintTop_toBottomOf="@+id/view5" />

    <Button
        android:id="@+id/YouIcon"
        android:layout_width="78dp"
        android:layout_height="78dp"
        android:background="@drawable/youicon"
        app:layout_constraintEnd_toEndOf="@+id/notificationIcon"
        app:layout_constraintTop_toBottomOf="@+id/view5" />

    <Button
        android:id="@+id/MarkedIcon"
        android:layout_width="78dp"
        android:layout_height="78dp"
        android:layout_marginStart="20dp"
        android:background="@drawable/markedicon"
        app:layout_constraintStart_toEndOf="@+id/homeIcon"
        app:layout_constraintTop_toBottomOf="@+id/view5" />

    <Button
        android:id="@+id/ScheduleIcon"
        android:layout_width="78dp"
        android:layout_height="78dp"
        android:background="@drawable/scheduleicon"
        app:layout_constraintEnd_toStartOf="@+id/YouIcon"
        app:layout_constraintStart_toEndOf="@+id/MarkedIcon"
        app:layout_constraintTop_toBottomOf="@+id/view5" />

</androidx.constraintlayout.widget.ConstraintLayout>